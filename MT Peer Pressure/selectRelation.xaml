<Window x:Class="MT_Peer_Pressure.selectRelation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		UseLayoutRounding="True" TextOptions.TextFormattingMode="Display" 
		Title="SelectRelationWindow" SizeToContent="WidthAndHeight" ShowInTaskbar="False" WindowStyle="None" ResizeMode="NoResize">
	<Border BorderBrush="Black" BorderThickness="1" Padding="2" SnapsToDevicePixels="True">
		<Grid>
			<StackPanel  HorizontalAlignment="Center" Name="stackPanel1" VerticalAlignment="Top" Orientation="Horizontal">
				<ComboBox Height="23" Name="comboBox1" Width="120" />
				<Button Content="Select" Height="23" Name="button1" Width="75" Click="button1_Click" />
			</StackPanel>
		</Grid>
	</Border>
</Window>